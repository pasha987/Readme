db.hospitals.find(
    { specializations: "Pediatric" },
    { name: 1, _id: 0 }
)
db.hospitals.find(
    { city: "Nashik" },
    { name: 1, _id: 0 }
)
db.doctors.findOne({ name: "Dr. Deshmukh" })
db.hospitals.find(
    { doctors: 1 },
    { name: 1, _id: 0 }
)
db.hospitals.find(
    { rating: { $gte: 4 } },
    { name: 1, _id: 0 }
)
